<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
         <hibernate-mapping>
        	 <class name="entity.Place" table="t_place"> 
        	 	<!-- 配置属性 -->
        	 	<id name="pid" column="pid">
        	 	<!-- 增长策略 -->
    	     	<generator class="native"></generator>  
     	 	</id>
        	 	<property name="pname"  column="pname"  ></property>   
        	 	<property name="pnumN"  column="pnumN" ></property>   
              	<property name="pnumS"  column="pnumS" ></property>     	 	      	 	
       	 	    <property name="prank"  column="prank" ></property>   
        	 </class>
</hibernate-mapping>